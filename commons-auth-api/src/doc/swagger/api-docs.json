{
  "swagger": "2.0",
  "info": {
    "description": "Handles authentication + authorization with registration-, invite and password-reset flows.",
    "version": "3.1.0",
    "title": "commons-auth api",
    "contact": {
      "name": "rocketbase.io",
      "url": "https://www.rocketbase.io",
      "email": "info@rocketbase.io"
    }
